    "import-in-the-middle": "^1.14.4",
    "ioredis": "^5.8.1",
    "kysely": "0.28.7",
    "mime-types": "^3.0.1",
    "nodemailer": "^7.0.7",
    "pg": "^8.16.3",
    "pg-boss": "^11.0.5",
    "pino": "9.13.1",
    "pino-pretty": "13.1.1",
    "postgres-interval": "^4.0.2",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tiny-invariant": "1.3.3",
    "tsx": "4.20.6",
    "zod": "4.1.11"
  }
}

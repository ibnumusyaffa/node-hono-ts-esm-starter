{
  "name": "my-app",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "dev": "tsx --watch src/server.ts | pino-pretty",
    "dev:email": "email dev --dir ./src/emails",
    "start": "node ./build/server.js",
    "start:otel": "node --import ./build/otel.js ./build/server.js",
    "build": "rimraf build && tsc && tsc-alias",
    "test": "vitest run --reporter=verbose",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "format:all": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|jsx|tsx)\"",
    "format:check": "prettier --ignore-path .gitignore --check \"**/*.+(js|ts|jsx|tsx)\"",
    "db:migrate": "kysely migrate:latest",
    "db:migrate:auth": "pnpm dlx @better-auth/cli migrate",
    "db:codegen": "kysely-codegen --dialect postgres --out-file ./src/lib/db/types.ts --default-schema=public",
    "knip": "knip"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "9.37.0",
    "@react-email/preview-server": "^4.2.12",
    "@types/mime-types": "^3.0.1",
    "@types/node": "24.7.0",
    "@types/nodemailer": "^7.0.2",
    "@types/pg": "^8.15.5",
    "@vitest/eslint-plugin": "1.3.16",
    "@vitest/ui": "^3.2.4",
    "eslint-config-prettier": "10.1.8",
    "knip": "^5.64.2",
    "kysely-codegen": "0.19.0",
    "kysely-ctl": "0.19.0",
    "prettier": "3.6.2",
    "react-email": "4.2.12",
    "rimraf": "6.0.1",
    "testcontainers": "11.7.1",
    "tsc-alias": "1.8.16",
    "typescript": "5.9.3",
    "typescript-eslint": "8.45.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@dotenvx/dotenvx": "1.51.0",
    "@faker-js/faker": "10.0.0",
    "@flystorage/aws-s3": "^1.2.0",
    "@flystorage/file-storage": "^1.2.0",
    "@flystorage/local-fs": "^1.2.0",
    "@hono/node-server": "1.19.5",
    "@hono/otel": "0.5.0",
    "@hono/zod-validator": "0.7.3",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-node": "0.64.6",
    "@opentelemetry/exporter-logs-otlp-http": "0.205.0",
    "@opentelemetry/exporter-metrics-otlp-http": "0.205.0",
    "@opentelemetry/exporter-trace-otlp-http": "0.205.0",
    "@opentelemetry/instrumentation-http": "0.205.0",
    "@opentelemetry/instrumentation-pg": "^0.58.3",
    "@opentelemetry/instrumentation-pino": "0.52.3",
    "@opentelemetry/instrumentation-winston": "0.50.2",
    "@opentelemetry/resources": "2.1.0",
    "@opentelemetry/sdk-logs": "0.205.0",
    "@opentelemetry/sdk-metrics": "2.1.0",
    "@opentelemetry/sdk-node": "0.205.0",
    "@opentelemetry/semantic-conventions": "1.37.0",
    "@react-email/components": "0.5.6",
    "@t3-oss/env-core": "0.13.8",
    "bentocache": "^1.5.0",
    "better-auth": "1.3.26",
    "commander": "14.0.1",
    "eslint-plugin-unicorn": "61.0.2",

{
  "name": "my-app",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "dev": "tsx --watch src/server.ts | pino-pretty",
    "dev:email": "email dev --dir ./src/emails",
    "start": "node ./dist/server.js",
    "start:otel": "node --import ./dist/otel.js ./dist/server.js",
    "build": "rimraf dist && tsc && tsc-alias",
    "test": "vitest run --reporter=verbose",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "format:all": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|jsx|tsx)\"",
    "format:check": "prettier --ignore-path .gitignore --check \"**/*.+(js|ts|jsx|tsx)\"",
    "db:migrate": "kysely migrate:latest",
    "db:migrate:auth": "npx @better-auth/cli migrate",
    "db:codegen": "kysely-codegen --dialect postgres --out-file ./src/lib/db/types.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "9.36.0",
    "@react-email/preview-server": "^4.2.11",
    "@types/node": "24.5.2",
    "@types/nodemailer": "^7.0.1",
    "@vitest/eslint-plugin": "1.3.12",
    "@vitest/ui": "^3.2.4",
    "eslint-config-prettier": "10.1.8",
    "kysely-codegen": "0.19.0",
    "kysely-ctl": "0.19.0",
    "prettier": "3.6.2",
    "react-email": "4.2.11",
    "rimraf": "6.0.1",
    "testcontainers": "11.6.0",
    "tsc-alias": "1.8.16",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "1.50.1",
    "@faker-js/faker": "10.0.0",
    "@hono/node-server": "1.19.3",
    "@hono/otel": "0.5.0",
    "@hono/zod-validator": "0.7.3",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-node": "0.64.1",
    "@opentelemetry/exporter-logs-otlp-http": "0.205.0",
    "@opentelemetry/exporter-metrics-otlp-http": "0.205.0",
    "@opentelemetry/exporter-trace-otlp-http": "0.205.0",
    "@opentelemetry/instrumentation-http": "0.205.0",
    "@opentelemetry/instrumentation-pg": "^0.58.0",
    "@opentelemetry/instrumentation-pino": "0.52.0",
    "@opentelemetry/instrumentation-winston": "0.50.0",
    "@opentelemetry/resources": "2.1.0",
    "@opentelemetry/sdk-logs": "0.205.0",
    "@opentelemetry/sdk-metrics": "2.1.0",
    "@opentelemetry/sdk-node": "0.205.0",
    "@opentelemetry/semantic-conventions": "1.37.0",
    "@react-email/components": "0.5.3",
    "@t3-oss/env-core": "0.13.8",
    "@types/pg": "^8.15.5",
    "bentocache": "^1.5.0",
    "better-auth": "1.3.13",
    "commander": "14.0.1",
    "eslint-plugin-unicorn": "61.0.2",
    "hono": "4.9.8",
    "kysely": "0.28.7",
    "nodemailer": "^7.0.6",
    "pg": "^8.16.3",
    "pg-boss": "^10.3.3",
    "pino": "9.11.0",
    "pino-pretty": "13.1.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tiny-invariant": "1.3.3",
    "tsx": "4.20.5",
    "winston": "3.17.0",
    "zod": "4.1.9"
  }

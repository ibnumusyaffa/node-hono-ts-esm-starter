{
  "name": "MyApp",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "tsx --env-file .env --watch src/server.ts | pino-pretty",
    "start": "node --env-file .env ./dist/server.js",
    "start:otel": "node --env-file .env --import ./dist/otel.js ./dist/server.js",
    "build": "rimraf ./dist && tsc && tsc-alias",
    "test": "dotenvx run -f .env.test -q -- vitest run --reporter=verbose",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "format:all": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|jsx|tsx)\"",
    "format:check": "prettier --ignore-path .gitignore --check \"**/*.+(js|ts|jsx|tsx)\"",
    "db:migrate": "kysely migrate:latest",
    "db:migrate:auth": "npx @better-auth/cli migrate",
    "db:codegen": "kysely-codegen --out-file ./src/lib/db/types.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "9.35.0",
    "@types/bcrypt": "6.0.0",
    "@types/node": "24.3.1",
    "@vitest/eslint-plugin": "1.3.9",
    "eslint-config-prettier": "10.1.8",
    "kysely-codegen": "0.19.0",
    "kysely-ctl": "0.19.0",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "testcontainers": "11.5.1",
    "tsc-alias": "1.8.16",
    "typescript": "5.9.2",
    "typescript-eslint": "8.43.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "1.49.0",
    "@faker-js/faker": "10.0.0",
    "@hono/node-server": "1.19.2",
    "@hono/otel": "0.5.0",
    "@hono/zod-validator": "0.7.2",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-node": "0.64.0",
    "@opentelemetry/exporter-logs-otlp-http": "0.205.0",
    "@opentelemetry/exporter-metrics-otlp-http": "0.205.0",
    "@opentelemetry/exporter-trace-otlp-http": "0.205.0",
    "@opentelemetry/instrumentation-http": "0.205.0",
    "@opentelemetry/instrumentation-mysql2": "0.52.0",
    "@opentelemetry/instrumentation-pino": "0.52.0",
    "@opentelemetry/instrumentation-winston": "0.50.0",
    "@opentelemetry/resources": "2.1.0",
    "@opentelemetry/sdk-logs": "0.205.0",
    "@opentelemetry/sdk-metrics": "2.1.0",
    "@opentelemetry/sdk-node": "0.205.0",
    "@opentelemetry/semantic-conventions": "1.37.0",
    "@t3-oss/env-core": "0.13.8",
    "bcrypt": "6.0.0",
    "better-auth": "1.3.9",
    "commander": "14.0.0",
    "eslint-plugin-unicorn": "61.0.2",
    "hono": "4.9.6",
    "kysely": "0.28.5",
    "mysql2": "3.14.5",
    "pino": "9.9.5",
    "pino-pretty": "13.1.1",
    "tiny-invariant": "1.3.3",
    "tsx": "4.20.5",
    "winston": "3.17.0",
    "zod": "4.1.5"
  }
}
